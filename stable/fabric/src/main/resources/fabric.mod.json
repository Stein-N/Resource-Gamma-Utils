{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [ "${mod_author}" ],
    "contact": {
        "homepage": "https://github.com/Stein-N",
        "sources": "https://github.com/Stein-N/Resource-Gamma-Utils"
    },
    "license": "${mod_license}",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [ "net.xstopho.resource_gamma_util.ResourceGammaUtils" ]
    },
    "mixins": [ "${mod_id}.mixins.json", "${mod_id}.compat.mixins.json" ],
    "depends": {
        "fabric-api": ">=${fabric_api}",
        "fabricloader": ">=${fabric_loader}",
        "minecraft": "${minecraft_version}",
        "java": ">=${java_version}"
    }
}